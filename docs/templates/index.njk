<html>
  <head>
    {% include "./head.njk" %}
  </head>
  <body>
    {% include "./header.njk" %}

    <!-- Main section-->
    <main class="container px-4">
      <div class="row gx-4 justify-content-center">
        <div class="col-lg-8">
          {% block title %}
          <h2>{{title}}</h2>
          {% endblock %}
          <div id="about">
            {% block content %}
            <ol>
            {% for rule in allRules %}
              <li>{{rule.rule_id}}</li>
            {% endfor %}
            </ol>
            {% endblock %}
          </div>
        </div>
      </div>
    </main>
    {% include "./footer.njk" %}
    {% include "./scripts.njk" %}

  </body>
</html>
